# Demo-friendly defaults for local walkthroughs
DEMO_MODE=true
NODE_ENV=development
PORT=5173

# Supabase (use local stack or temporary project)
SUPABASE_URL=http://localhost:54321
SUPABASE_ANON_KEY=demo-anon-key
SUPABASE_SERVICE_ROLE_KEY=demo-service-role-key
VITE_SUPABASE_URL=http://localhost:54321
VITE_SUPABASE_ANON_KEY=demo-anon-key

# SYMBI defaults for demo datasets
VITE_TENANT_ID=demo-tenant
VITE_MODEL_ID=gpt-4o
VITE_POLICY_PACK=baseline-demo
RETENTION_DAYS=30

# Security & rate limiting
ENABLE_PERSISTENT_RATE_LIMITS=false
RATE_LIMIT_CAPACITY=20
RATE_LIMIT_RPS=5

# Optional signing keys (leave empty for demo)
VITE_ED25519_PUBLIC_KEY_BASE64=
VITE_ED25519_PRIVATE_KEY_BASE64=
VITE_ED25519_KEYS_JSON={"kid1":"base64pubkey=="}
ED25519_KEYS_JSON={"kid1":"base64pubkey=="}
