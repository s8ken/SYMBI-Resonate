{
  "name": "symbi-synergy",
  "version": "1.0.0",
  "description": "A zero-code, cloud-based, decentralized AI platform for SYMBI",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/server.js",
    "dev:frontend": "cd frontend && npm start",
    "install:all": "npm install && cd frontend && npm install",
    "build": "cd frontend && npm run build",
    "test": "jest"
  },
  "keywords": [
    "AI",
    "LLM",
    "decentralized",
    "SYMBI",
    "cloud"
  ],
  "author": "SYMBI Team",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.0.170",
    "mongoose": "^7.6.3",
    "morgan": "^1.10.0",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4"
  }
}
